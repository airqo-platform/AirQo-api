{
  "name": "@airqo/icons-react",
  "version": "0.2.7",
  "description": "Production-ready React icon library with 1,383+ TypeScript components and tree-shakable imports",
  "keywords": [
    "icons",
    "react",
    "nextjs",
    "typescript",
    "svg",
    "components",
    "airqo",
    "ui",
    "design-system",
    "tree-shaking"
  ],
  "homepage": "https://airqo.net/packages",
  "bugs": "https://github.com/airqo-platform/AirQo-api/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/airqo-platform/AirQo-api.git",
    "directory": "packages/airqo-icons/packages/react"
  },
  "license": "MIT",
  "author": "AirQo Organization",
  "sideEffects": [
    "./src/components/register-icons.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build:icons": "node ../../tools/generators/react-generator-enhanced.js && tsc --noEmit",
    "build:bundle": "tsup",
    "build": "npm run build:icons && npm run build:bundle",
    "dev": "npm run build:icons && tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo 'React package: TypeScript compilation handles linting'",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rimraf dist src/components"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "rimraf": "^5.0.6",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "fuse.js": "^7.1.0"
  }
}
