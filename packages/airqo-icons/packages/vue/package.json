{
  "name": "@airqo/icons-vue",
  "version": "0.2.3",
  "description": "Production-ready Vue icon library with 1,383+ TypeScript components and tree-shakable imports",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:icons": "node ../../tools/generators/vue-generator.js",
    "build:bundle": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "build": "npm run build:icons && npm run build:bundle",
    "dev": "npm run build:icons && vite build --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "icons",
    "vue",
    "vue3",
    "nuxt",
    "typescript",
    "svg",
    "components",
    "airqo",
    "ui",
    "design-system",
    "tree-shaking"
  ],
  "homepage": "https://airqo.net/packages",
  "bugs": "https://github.com/airqo-platform/AirQo-api/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/airqo-platform/AirQo-api.git",
    "directory": "packages/airqo-icons/packages/vue"
  },
  "license": "MIT",
  "author": "AirQo Organization",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/ui": "^0.34.6",
    "@vue/test-utils": "^2.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "typescript": "^5.4.0",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  }
}
